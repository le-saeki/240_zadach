#include <iostream>
using namespace std;

int maxmin1(int*arr, int first, int last)
{
	int min = arr[0];
	
	for(; first<last;++first)
	{
		if(arr[first]<min && arr[first]>0)
	{	 
		min=arr[first];
	}
	
	    if(min<0)
	    { return 0;
	   	}
	}
 	return min;

}

int main()
{
	int array[5]{-4,-3,-6,-4,-5};
	cout<<maxmin1(array,0,5);
	return 0;
}
