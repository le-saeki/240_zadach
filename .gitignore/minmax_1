#include <iostream>
using namespace std;

int maxmin1(int*arr, int first, int last)
{
	int max = arr[0];
	int min = arr[0];
	
	for(; first<last;++first)
	{
		if(arr[first]>max)
	{	 
		max=arr[first];
	}
	}
	cout<< max<<endl;
	
	for(; first<last;++first)
	{
		if(arr[first]<min)
	{	 
		min=arr[first];
	}
	}

	
	return min ;

}

int main()
{
	int array[5]{2,3,56,4,3};
	cout<<maxmin1(array,0,5);
	return 0;
}
